{
  "name": "Chrome header toggler",
  "description": "Chrome header toggler",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "background": {
    "page":"html/background.html"
  },
  "options_page": "html/options.html",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "background",
    "tabs",
    "storage"
  ],
  "icons": {
    "128": "img/build_72.png"
  },
  "page_action": {
    "default_icon":{
      "36": "img/build_38.png"
    },
    "default_title": "Toggle Dev mode", 
    "default_popup": "html/popup.html"
  }
}